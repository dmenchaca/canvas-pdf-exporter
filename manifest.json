{
  "manifest_version": 3,
  "name": "Canvas PDF Exporter",
  "version": "1.8",
  "description": "Exporteer Canvas e-learning pagina's naar PDF",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
